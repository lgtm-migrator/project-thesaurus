{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "strict": false,
    "module": "esnext",
    "moduleResolution": "node",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/vuetify/types"
    ],
    "types": [
      "vuetify"
    ],
    "lib": [
      "es2015",
      "dom"
    ],
    "allowSyntheticDefaultImports": true
  }
}
